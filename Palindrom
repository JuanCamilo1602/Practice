public class Palindrom {
	BufferedWriter bw = new BufferedWriter (new OutputStreamWriter(System.out));
	
	public static boolean palindrom(String string){
		string = string.replaceAll("", "");
		for(int a = 0; a <string.length();a++) {
			if(string.charAt(a)!= string.charAt(string.length()-a-1)){
				return false;
				
			}
		}
		return true;
		
	}
	bw.write("Type string of characters");
	bw.newLine();
	bw.flush();
	string=(br.reaLine());
	if(void palindrom (string) == true) {
		bw.write("The string is palindrom");
		bw.newLine();
	}else {
		bw.write("THe string isn't palindrom");
		bw.newLine();
	}
	bw.flush();
	break;
}
